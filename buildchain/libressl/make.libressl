${FETCHCMD} http://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-2.8.2.tar.gz
SRCDIR=libressl-2.8.2
${RMCMD} $SRCDIR
${TARCMD} -vxf *.tar.gz
../patcher
cd $SRCDIR
${MKDIRCMD} build
cd build
../configure --prefix=${FINPREFIX} --host=$MACHARCH --target=$MACHARCH \
	--build=$MACHARCH --disable-hardening &&
${MKCMD} &&
${MKCMD} install
