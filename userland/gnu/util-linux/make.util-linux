${FETCHCMD} https://mirrors.edge.kernel.org/pub/linux/utils/util-linux/v2.33/util-linux-2.33.tar.xz
SRCDIR=util-linux-2.33
${RMCMD} $SRCDIR
${TARCMD} -vxf *.tar.xz
../../patcher
cd $SRCDIR
