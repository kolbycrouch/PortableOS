${FETCHCMD} https://ftp.gnu.org/gnu/nano/nano-3.2.tar.xz
SRCDIR=nano-3.2
${RMCMD} $SRCDIR
${TARCMD} -vxf *.tar.xz
../../patcher
cd $SRCDIR
